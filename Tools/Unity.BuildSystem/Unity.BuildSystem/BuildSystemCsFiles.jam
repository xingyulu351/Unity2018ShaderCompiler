
rule BuildSystemCsFiles {
    if $(gBuildSystemCsFiles)
    {
        return $(gBuildSystemCsFiles) ;
    }

    #Some buildsystem .jam.cs files we find through globbing reasonable directories where we expect to find them.
    local globDirectories =
        Modules
        Projects/Jam
        Platforms
    ;
    gBuildSystemCsFiles = ;

    for dir in $(globDirectories)
    {
        gBuildSystemCsFiles += @($(dir)/**/*.jam.cs:W) ;
    }

    #in addition to globbing, we also specify a bunch of rarely changing ones directly. we do not glob these, as it would require globbing all of External/ which is quite expensive.
