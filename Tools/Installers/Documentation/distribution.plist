<?xml version="1.0" encoding="utf-8" standalone="no"?>
<installer-gui-script minSpecVersion="1">
	<title>Unity Documentation</title>
    <pkg-ref id="com.unity3d.Documentation"/>
    <options hostArchitectures='i386'/>
    <options customize="never" require-scripts="true"/>
    <background file="background.tiff" alignment="topleft"/>
    <welcome file="Welcome.rtf"/>
    <choices-outline>
            <line choice="com.unity3d.Documentation"/>
    </choices-outline>
    <choice id="com.unity3d.Documentation" visible="false">
        <pkg-ref id="com.unity3d.Documentation" auth='Root'/>
    </choice>
    <pkg-ref id="com.unity3d.Documentation" version="0" onConclusion="none">Documentation.pkg.tmp</pkg-ref>
</installer-gui-script>
